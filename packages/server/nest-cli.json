{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "builder": "swc",
    "typeCheck": true
  },
  "watchOptions": {
    "watchAssets": true,
    "usePolling": false,
    "interval": 100
  }
}
